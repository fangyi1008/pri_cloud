<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hontosec.cloud.network.dao.SecurityGroupVmDao">
<select id="queryByGroupId" resultType="com.hontosec.cloud.network.entity.SecurityGroupVmEntity">
	select * from security_group_vm_table where security_group_id = #{securityGroupId}
</select>
<select id="queryByVmId" resultType="com.hontosec.cloud.network.entity.SecurityGroupVmEntity">
	select * from security_group_vm_table where vm_id = #{vmId}
</select>
</mapper>